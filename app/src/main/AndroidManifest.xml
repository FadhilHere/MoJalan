<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mojalan">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoJalan">

        <!-- Aktivitas utama yang diluncurkan saat aplikasi dimulai -->
        <activity android:name=".MainActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Aktivitas lain dalam aplikasi -->
        <activity android:name=".NotificationActivity" android:exported="false" />
        <activity android:name=".ChatActivity" android:exported="false" />
        <activity android:name=".OnboardingActivity" android:exported="false" />
        <activity android:name=".HomeActivity" android:exported="false" />
        <activity android:name=".LoginActivity" android:exported="false" />
        <activity android:name=".RegisterActivity" android:exported="false" />

        <!-- Aktivitas baru yang ditambahkan -->
        <activity android:name=".Rencana2" android:exported="false" />
        <activity android:name=".Rencana3" android:exported="false" />
        <activity android:name=".Rencana31" android:exported="false" />
        <activity android:name=".Rencana4Activity" android:exported="false" />
        <activity android:name=".Rencana5Activity" android:exported="false" />
        <activity android:name=".Rencana6Activity" android:exported="false" />
        <activity android:name=".Rencana7Activity" android:exported="false" />
        <activity android:name=".Rencana8Activity" android:exported="false" />
        <activity android:name=".ProfileActivity" android:exported="false" />
        <activity android:name=".EditProfileActivity" android:exported="false" />
        <activity android:name=".UlasanActivity" android:exported="false" />
        <activity android:name=".PesananUserActivity" android:exported="false" />

    </application>
</manifest>
